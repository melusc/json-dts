{
	"author": {
		"name": "lusc",
		"url": "https://github.com/melusc"
	},
	"ava": {
		"files": [
			"test/**/*"
		],
		"typescript": {
			"compile": false,
			"extensions": [
				"ts"
			],
			"rewritePaths": {
				"test/": "dist/test/"
			}
		}
	},
	"exports": {
		".": "./dist/src/index.js"
	},
	"packageManager": "yarn@4.9.1",
	"description": "Convert json to typescript dts",
	"devDependencies": {
		"@ava/typescript": "^5.0.0",
		"@fast-check/ava": "^2.0.2",
		"@lusc/clean-package": "^2.1.1",
		"@lusc/eslint-config": "^7.1.1",
		"@lusc/tsconfig": "^6.0.2",
		"@types/node": "^22.15.2",
		"ava": "^6.2.0",
		"eslint": "^9.25.1",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": "^20.19.1 || ^22.15.0 || ^23.11.0"
	},
	"files": [
		"./dist/src"
	],
	"keywords": [
		"json",
		"types",
		"typescript",
		"declaration",
		"dts"
	],
	"license": "MIT",
	"name": "@lusc/json-dts",
	"repository": {
		"type": "git",
		"url": "https://github.com/melusc/json-dts"
	},
	"scripts": {
		"fmt": "prettier -w . && eslint --fix",
		"test": "yarn build && ava",
		"build": "yarn clean && tsc",
		"clean": "rimraf dist"
	},
	"type": "module",
	"version": "4.2.3",
	"dependencies": {
		"camelcase": "^8.0.0"
	}
}
