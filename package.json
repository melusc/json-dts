{
	"author": {
		"name": "lusc",
		"url": "https://github.com/melusc"
	},
	"ava": {
		"files": [
			"test/**/*"
		],
		"typescript": {
			"compile": false,
			"extensions": [
				"ts"
			],
			"rewritePaths": {
				"test/": "dist/test/"
			}
		}
	},
	"exports": {
		".": "./dist/src/index.js"
	},
	"packageManager": "yarn@3.4.1",
	"description": "Convert json to typescript dts",
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@fast-check/ava": "^1.1.3",
		"@lusc/tsconfig": "^2.1.0",
		"@types/node": "^18.13.0",
		"ava": "^5.2.0",
		"prettier": "^2.8.4",
		"rimraf": "^4.1.2",
		"typescript": "^4.9.5",
		"xo": "^0.53.1"
	},
	"engines": {
		"node": "^16.9.0 || >=18"
	},
	"files": [
		"./dist/src"
	],
	"keywords": [
		"json",
		"types",
		"typescript",
		"declaration",
		"dts"
	],
	"license": "MIT",
	"name": "@lusc/json-dts",
	"repository": "melusc/json-dts",
	"scripts": {
		"fmt": "prettier -w . && xo --fix",
		"test": "yarn build && ava",
		"build": "yarn clean && tsc",
		"clean": "rimraf dist"
	},
	"type": "module",
	"version": "2.0.0",
	"dependencies": {
		"camelcase": "^7.0.1",
		"type-fest": "^3.5.7"
	},
	"publishConfig": {
		"access": "public"
	}
}
