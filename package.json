{
	"author": {
		"name": "lusc",
		"url": "https://github.com/melusc"
	},
	"ava": {
		"files": [
			"test/**/*"
		],
		"typescript": {
			"compile": false,
			"extensions": [
				"ts"
			],
			"rewritePaths": {
				"test/": "dist/test/"
			}
		}
	},
	"exports": {
		".": "./dist/src/index.js"
	},
	"packageManager": "yarn@4.0.2",
	"description": "Convert json to typescript dts",
	"devDependencies": {
		"@ava/typescript": "^4.1.0",
		"@fast-check/ava": "^1.1.6",
		"@lusc/tsconfig": "^4.0.0",
		"@types/node": "^20.9.0",
		"ava": "^5.3.1",
		"prettier": "^3.1.0",
		"rimraf": "^5.0.5",
		"typescript": "^5.2.2",
		"xo": "^0.56.0"
	},
	"engines": {
		"node": "^18.17.1 || ^20.6.1"
	},
	"files": [
		"./dist/src"
	],
	"keywords": [
		"json",
		"types",
		"typescript",
		"declaration",
		"dts"
	],
	"license": "MIT",
	"name": "@lusc/json-dts",
	"repository": "melusc/json-dts",
	"scripts": {
		"fmt": "prettier -w . && xo --fix",
		"test": "yarn build && ava",
		"build": "yarn clean && tsc",
		"clean": "rimraf dist"
	},
	"type": "module",
	"version": "4.0.0",
	"dependencies": {
		"camelcase": "^8.0.0",
		"type-fest": "^4.7.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
