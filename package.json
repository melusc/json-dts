{
	"author": {
		"name": "lusc",
		"url": "https://github.com/melusc"
	},
	"ava": {
		"files": [
			"test/**/*"
		],
		"typescript": {
			"compile": false,
			"extensions": [
				"ts"
			],
			"rewritePaths": {
				"test/": "dist/test/"
			}
		}
	},
	"exports": {
		".": "./dist/src/index.js"
	},
	"packageManager": "yarn@4.5.1",
	"description": "Convert json to typescript dts",
	"devDependencies": {
		"@ava/typescript": "^5.0.0",
		"@fast-check/ava": "^2.0.1",
		"@lusc/eslint-config": "^2.0.4",
		"@lusc/tsconfig": "^5.1.0",
		"@types/node": "^22.8.5",
		"ava": "^6.2.0",
		"eslint": "^9.14.0",
		"prettier": "^3.3.3",
		"rimraf": "^6.0.1",
		"typescript": "^5.6.3"
	},
	"engines": {
		"node": "^18.17.1 || ^20.6.1 || ^22.0.0"
	},
	"files": [
		"./dist/src"
	],
	"keywords": [
		"json",
		"types",
		"typescript",
		"declaration",
		"dts"
	],
	"license": "MIT",
	"name": "@lusc/json-dts",
	"repository": "melusc/json-dts",
	"scripts": {
		"fmt": "prettier -w . && eslint --fix",
		"test": "yarn build && ava",
		"build": "yarn clean && tsc",
		"clean": "rimraf dist"
	},
	"type": "module",
	"version": "4.1.1",
	"dependencies": {
		"camelcase": "^8.0.0",
		"type-fest": "^4.26.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
