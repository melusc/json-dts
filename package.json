{
	"author": {
		"name": "lusc",
		"url": "https://github.com/melusc"
	},
	"ava": {
		"environmentVariables": {
			"NODE_ENV": "AVA_TESTING",
			"files": [
				"test/**/*"
			],
			"typescript": {
				"extensions": [
					"ts"
				],
				"rewritePaths": {
					"test/": "dist/test"
				}
			}
		}
	},
	"dependencies": {
		"kind-of": "^6.0.3"
	},
	"description": "Summarise json as typescript type declaration",
	"devDependencies": {
		"@ava/typescript": "^1.1.1",
		"@sindresorhus/tsconfig": "^2.0.0",
		"ava": "^3.15.0",
		"prettier": "^2.4.0",
		"type-fest": "^2.2.0",
		"typescript": "^4.4.2",
		"xo": "^0.44.0"
	},
	"engines": {
		"node": ">=12.20"
	},
	"files": [
		"./dist"
	],
	"keywords": [
		"json",
		"types",
		"typescript",
		"declaration"
	],
	"license": "MIT",
	"name": "@lusc/json-type",
	"repository": "melusc/json-type",
	"scripts": {
		"lint": "prettier -w \"*.json\" .prettierrc \"{src,test}/**/*\" && xo --fix \"src/**/*.ts\"",
		"test": "xo && ava"
	},
	"type": "module",
	"version": "0.0.1"
}
