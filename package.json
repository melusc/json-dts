{
	"author": {
		"name": "lusc",
		"url": "https://github.com/melusc"
	},
	"ava": {
		"environmentVariables": {
			"NODE_ENV": "AVA_TESTING"
		},
		"files": [
			"test/**/*"
		],
		"typescript": {
			"compile": false,
			"extensions": [
				"ts"
			],
			"rewritePaths": {
				"test/": "dist/test/"
			}
		}
	},
	"exports": {
		".": "./dist/src/index.js"
	},
	"packageManager": "yarn@3.2.4",
	"description": "Summarise json as typescript type declaration",
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@lusc/tsconfig": "^1.1.0",
		"ava": "^4.2.0",
		"prettier": "^2.6.2",
		"type-fest": "^2.12.2",
		"typescript": "^4.9.3",
		"xo": "^0.49.0"
	},
	"engines": {
		"node": ">=12.20"
	},
	"files": [
		"./dist/src"
	],
	"keywords": [
		"json",
		"types",
		"typescript",
		"declaration"
	],
	"license": "MIT",
	"name": "@lusc/json-type",
	"repository": "melusc/json-type",
	"scripts": {
		"fmt": "prettier -w . && xo --fix",
		"test": "yarn build && ava",
		"build": "tsc"
	},
	"type": "module",
	"version": "0.0.1"
}
