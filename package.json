{
	"author": {
		"name": "lusc",
		"url": "https://github.com/melusc"
	},
	"ava": {
		"files": [
			"test/**/*"
		],
		"typescript": {
			"compile": false,
			"extensions": [
				"ts"
			],
			"rewritePaths": {
				"test/": "dist/test/"
			}
		}
	},
	"exports": {
		".": "./dist/src/index.js"
	},
	"packageManager": "pnpm@10.30.2+sha512.36cdc707e7b7940a988c9c1ecf88d084f8514b5c3f085f53a2e244c2921d3b2545bc20dd4ebe1fc245feec463bb298aecea7a63ed1f7680b877dc6379d8d0cb4",
	"description": "Convert json to typescript dts",
	"devDependencies": {
		"@ava/typescript": "^6.0.0",
		"@fast-check/ava": "^2.0.2",
		"@lusc/clean-package": "^3.0.0",
		"@lusc/eslint-config": "^9.0.0",
		"@lusc/tsconfig": "^7.2.0",
		"@types/node": "^24.10.13",
		"ava": "^6.4.1",
		"eslint": "^10.0.2",
		"prettier": "^3.8.1",
		"rimraf": "^6.1.3",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": "^22.15.0 || ^24.6.0 || ^25.0.0"
	},
	"files": [
		"./dist/src"
	],
