		"json",
		"types",
		"typescript",
		"declaration",
		"dts"
	],
	"license": "MIT",
	"name": "@lusc/json-dts",
	"repository": {
		"type": "git",
		"url": "https://github.com/melusc/json-dts"
	},
	"scripts": {
		"fmt": "prettier -w . && eslint --fix",
		"test": "pnpm build && ava",
		"build": "pnpm clean && tsc",
		"clean": "rimraf dist"
	},
	"type": "module",
	"version": "7.0.0",
	"dependencies": {
		"camelcase": "^9.0.0"
	}
}
