	"scripts": {
		"fmt": "prettier -w . && eslint --fix",
		"test": "pnpm build && ava",
		"build": "pnpm clean && tsc",
		"clean": "rimraf dist"
	},
	"type": "module",
	"version": "6.1.0",
	"dependencies": {
		"camelcase": "^8.0.0"
	}
